<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>login-form</title>
    <style>
      
.body {
  background-color: #289a09;
  font-family: Arial, sans-serif;
}

.h1 {
    padding-left: 20px;
  color: #d75d11;
}

.div {
  border: 1px solid #6c0b89;
  padding: 20px;
  margin: 20px;
  border-radius: 10px;
}

.p {
  margin: 5px 0;
}

    </style>
</head>

<body class="body">
    <h1 class="h1">Profile</h1>
    <div class="div">
        <p class="p">Name: <%= data.firstName %>
        </p>
        <p class="p">Email: <%= data.email %>
        </p>
        <p class="p">Phone: <%= data.mobile %>
        </p>
        <p class="p">id: <%= data.pkUserId %>
        </p>
    </div>
</body>

<script>
    let u = {
        'id': '<%= data.pkUserId %>',
        'name': '<%= data.firstName %>',
        'email': '<%= data.email %>',
        'mobile': '<%= data.mobile %>',
        'password': '<%= data.password %>'
    };
    console.log('uuuuuuuuuuuu', u);
    console.log('sssssssssss',localStorage.getItem('userData'));
    if (!localStorage.getItem('userData')) {
        localStorage.setItem('userData', JSON.stringify(u));
    }
</script>

</html>